{
    "env": {
        "es6": true
    },
    "extends": [
        "standard"
    ],
    "globals": {
      "__DEV__": true,
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [".js", ".jsx"]
        }
      ],
      "import/prefer-default-export": "off"
    }
}
